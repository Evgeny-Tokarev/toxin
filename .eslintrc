{
  "parser": "@babel/eslint-parser",

  "extends": ["airbnb-base", "prettier"],
  "rules": {
    "func-names": 0,
    "no-console": 0,
    "class-methods-use-this": [
      "error",
      { "exceptMethods": ["total", "isEmpty", "submit", "vividLabel"] }
    ]
  },
  "env": {
    "browser": true,
    "commonjs": true,
    "es6": true,
    "jquery": true
  }
}
